html {
    color: #333;
    background: #eef1f0;
}

input[type=button],
input[type=text],
input[type=password], 
input[type=number], 
input[type=search] {
    -webkit-appearance: none;
    outline: none;
    -webkit-appearance: textfield;
}

body,
div,
dl,
dt,
dd,
ul,
ol,
li,
h1,
h2,
h3,
h4,
h5,
h6,
pre,
form,
fieldset,
input,
textarea,
p,
blockquote,
th,
td,
section {
    margin: 0;
    padding: 0;
}

a {
    text-decoration: none;
        color: inherit;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

input {
    border: none;
}

fieldset,
img {
    display: block;
    border: 0;
    width: 100%;
}

address,
caption,
cite,
code,
dfn,
em,
strong,
th,
var {
    font-style: normal;
    font-weight: normal;
}

ol,
ul {
    list-style: none;
}

caption,
th {
    text-align: left;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    font-size: 100%;
    font-weight: normal;
}

q:before,
q:after {
    content: '';
}

abbr,
acronym {
    border: 0;
}

body {
    padding-bottom: 98px;
    font-size: 26px;
    font-family: Arial, Helvetica, sans-serif;
}

.cl:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.cl {
    display: inline-block;
}

.cl {
    display: block;
}

.swiper-container {
    width: 100%;
    height: 100%;
}

.swiper-pagination-bullet {
    height: 10px;
    width: 10px;
    background: rgba(255, 255, 255, .8) !important;
}

.swiper-pagination-bullet-active {
    width: 20px;
    border-radius: 10px;
    background: #fff;
}

.notice {
    background: #ffe8a2;
    height: 90px;
    position: relative
}

.notice p {
    padding: 0 57px 0 105px;
    line-height: 90px;
    color: #ff6d00;
    font-size: 26px;
    height: 90px;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.notice p:before {
    position: absolute;
    top: 30px;
    left: 57px;
    display: block;
    content: "";
    width: 30px;
    height: 30px;
    background: url(../images/icon-notice.png) no-repeat;
}

.nav {
    margin-bottom: 30px;
    width: 100%;
}

.nav {
    padding: 20px 0;
    background: #fff;
}

.nav li {
    float: left;
    width: 25%;
    text-align: center;
}

.nav li span {
    margin: 0 auto;
    margin-bottom: 10px;
    display: block;
    width: 130px;
    height: 130px;
    -webkit-border-radius: 65px;
    -moz-border-radius: 65px;
    border-radius: 65px;
    color: #fff;
    font-size: 34px;
    padding: 20px 30px;
    box-sizing: border-box;
}

.span_6_len{
    font-size: 24px !important;
}

.span_9_len{
    font-size: 22px !important;
}

.span_12_len{
    font-size: 20px !important;
}

.nav li:nth-of-type(4n - 3) span {
    background-color: #43cfa7;
}

.nav li:nth-of-type(4n - 2) span {
    background-color: #fbc02d;
}

.nav li:nth-of-type(4n - 1) span {
    background-color: #ff8a65;
}

.nav li:nth-of-type(4n + 0) span {
    background-color: #40c4ff;
}

.nav li p {
    font-size: 26px;
    color: #333;
}

.pro-box {
    margin-bottom: 20px;
    padding-bottom: 20px;
    background: #fff;
}

.pro-box h4 {
    padding: 0 40px 0 130px;
    line-height: 98px;
    height: 98px;
    background: #43cfa7;
    font-size: 34px;
    color: #fff;
    position: relative;
}

.pro-box h4:before {
    position: absolute;
    top: 20px;
    left: 40px;
    display: block;
    content: "";
    width: 60px;
    height: 60px;
    background: #64ebc4;
    border-radius: 100%;
}

.pro-list {
    padding: 20px 10px;
}

.pro-list li {
    float: left;
    margin-bottom: 20px;
    width: 50%;
    padding: 0 10px;
    box-sizing: border-box;
    position: relative;
}

.pro-list dl {
    border: 1px solid #dde3e1;
    background: #fff;
}

.pro-list li dt {
    border-bottom: 1px solid #dde3e1;
}

.pro-list li img {
    display: block;
    width: 342px;
    height: 258px;
}

.pro-list li h5 {
    padding: 20px 0;
    font-size: 26px;
    overflow: hidden;
    line-height: 1;
    height: 26px;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.pro-list .price {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 24px;
    color: #ff1744;
    line-height: 1;
}

.pro-list li dd {
    padding: 0 20px;
    padding-bottom: 18px;
}

.pro-list p{
    color: #90a4ae;
    font-size: 24px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}

.btn-add {
    position: absolute;
    bottom: 9px;
    right: 19px;
    display: block;
    width: 40px;
    height: 40px;
    background: url(../images/icon-add.png) no-repeat;
}

.pro-box .btn-more {
    margin: 0 auto;
    display: block;
    width: 220px;
    height: 60px;
    -webkit-border-radius: 30px;
    -moz-border-radius: 30px;
    border-radius: 30px;
    color: #43cfa7;
    font-size: 26px;
    line-height: 60px;
    text-align: center;
    background: #ecfaf6;
}

.links {
    /*margin-bottom: 30px;*/
    padding: 0 35px;
    background: #fff;
}

.links ul {
    border-bottom: 1px solid #dde3e1;
}

.links li {
    float: left;
    width: 50%;
    text-align: center;
}

.links a {
    display: block;
    padding: 37px 0;
    font-size: 26px;
    color: #333;
    line-height: 1;
}

.links p {
    padding: 30px 0;
    text-align: center;
    color: #cfd7d5;
    font-size: 24px;
}

footer {
    background: #fafafa;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    box-sizing: border-box;
}

footer ul {
    padding-left: 80px;
}

footer li {
    float: left;
    width: 33.3333%;
    border-left: 1px solid #bfbfbf;
    box-sizing: border-box;
}

footer li:first-of-type {
    position: absolute;
    width: 80px;
    left: 0;
    top: 0;
    background: url(../images/icon-home.png) center center no-repeat;
    border: none;
}

footer a {
    display: block;
    font-size: 26px;
    color: #333;
    line-height: 96px;
    height: 96px;
    text-align: center;
}


/*商品列表*/

.search {
    padding: 15px 20px;
    background: #cfd7d5;
}

.search input {
    width: 100%;
    height: 60px;
    padding: 16px 40px;
    background: #fff;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    font-size: 26px;
    border-radius: 60px;
}

.search input::-webkit-input-placeholder {
    color: #cfd7d5;
}

.search-result .pro-list {
    padding: 0 10px;
}

.search-result h3 {
    padding: 0 30px;
    font-size: 26px;
    color: #90a4ae;
    line-height: 90px;
}

.search-result .no-more {
    padding: 30px 0 50px;
}

.no-more {
    text-align: center;
    color: #90a4ae;
    font-size: 24px;
}


/*商品列表 购买弹窗*/

.full-bg {
    display: none;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    background: rgba(0, 0, 0, .4);
}

.cart-wrap {
    display: none;
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background: #fff;
    /* height: 511px; */
    z-index: 999;
}

.cart-box {
    position: relative;
}

.cart-box dt {
    position: relative;
    padding: 0 58px 0 205px;
    min-height: 130px;
}

.cart-box dt img {
    position: absolute;
    top: -23px;
    left: 17px;
    width: 150px;
    height: 150px;
    border-radius: 100%;
    display: block;
    border: 3px solid #fff;
}

.cart-box dt h5 {
    padding-top: 12px;
    font-size: 30px;
}

.cart-box dt p {
    padding-top: 10px;
    color: #90a4ae;
    font-size: 24px;
}

.cart-box dt .price {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 24px;
    color: #ec4343;
}

.cart-box dt .price em {
    font-size: 36px;
    font-weight: bold;
}

.cart-box .close {
    position: absolute;
    top: 0;
    right: 0;
    z-index: 100;
    display: block;
    width: 58px;
    height: 58px;
    background: url(../images/icon-close.png) no-repeat;
}

.cart-box dd {
    padding: 40px 20px 50px;
}

.cart-box dd h5 {
    padding-bottom: 18px;
    color: #90a4ae;
    font-size: 30px;
}

.spec-list li {
    float: left;
    padding: 0 20px 4px 0;
}

.spec-list li a {
    display: block;
    line-height: 50px;
    height: 50px;
    text-align: center;
    font-size: 24px;
    color: #90a4ae;
    background: #eef1f0;
    padding: 0 22px;
    border-radius: 50px;
    float: left;
    margin-right: 14px;
    margin-bottom: 15px;
}

.count-box {
    padding: 30px 10px 50px 0;
}

.count-box .count {
    display: block;
    float: left;
    font-size: 30px;
    line-height: 60px;
}

.count-right {
    float: right;
}

.count-right a,
.count-right span {
    float: right;
    display: block;
}

.count-right a {
    width: 60px;
    height: 60px;
}

.count-right .reduce {
    background: url(../images/count-reduce.png) no-repeat;
}

.count-right .add {
    background: url(../images/count-add.png) no-repeat;
}

.count-right span {
    text-align: center;
    padding-left: 10px;
    width: 70px;
    font-size: 30px;
    line-height: 60px;
    font-family: Arial, Helvetica, sans-serif;
}

.cart-footer {
    border-top: 1px solid #e7e7e7;
    border-bottom: 1px solid #e7e7e7;
    height: 97px;
    background: #fff;
}

.cart-footer-left {
    float: left;
    width: 250px;
    padding: 12px 0;
}

.cart-footer-left li {
    width: 100%;
    float: left;
    position: relative;
}

.cart-footer-left li i {
    display: block;
    margin: 0 auto;
    height: 42px;
}

.cart-footer-left li .icon-service {
    width: 43px;
    background: url(../images/icon-service.png) no-repeat;
}

.cart-footer-left li .icon-cart {
    width: 47px;
    position: relative;
    background: url(../images/icon-cart.png) no-repeat;
}

.cart-footer-left li p {
    line-height: 1;
    margin-top: 13px;
    color: #90a4ae;
    font-size: 20px;
    text-align: center;
}

.cart-footer-left li em {
    position: absolute;
    top: -8px;
    left: 13px;
    display: block;
    width: 30px;
    height: 30px;
    background: #ff0202;
    text-align: center;
    line-height: 30px;
    border-radius: 100%;
    font-size: 18px;
    color: #fff;
}

.shopcart_link em{
        position: absolute;
    top: -8px;
        right: 33%;
    display: block;
    width: 30px;
    height: 30px;
    background: #ff0202;
    text-align: center;
    line-height: 30px;
    border-radius: 100%;
    font-size: 18px;
    color: #fff;
}

.cart-footer .btn-addCart {
    float: right;
    width: 250px;
    height: 97px;
    background: #fbc02d;
    color: #fff;
    font-size: 36px;
    line-height: 97px;
    text-align: center;
}

.cart-footer .btn-buy {
    float: right;
    width: 250px;
    height: 97px;
    background: #43cfa7;
    color: #fff;
    font-size: 36px;
    line-height: 97px;
    text-align: center;
}


/*商品详情*/

.detail-top {
    background: #fff;
}

.detail-top img {
    display: block;
    width: 750px;
    height: 528px;
}

.detail-top h5 {
    padding: 28px 28px 10px;
    font-size: 30px;
    line-height: 34px;
    color: #363636;
}

.detail-top p {
    padding: 10px 28px;
    color: #90a4ae;
    font-size: 24px;
}

.detail-top .price {
    color: #ec4343;
    font-family: Arial, Helvetica, sans-serif;
}

.detail-top .price em {
    font-size: 36px;
    font-weight: bold;
}

.detail-top .old-price {
    font-family: Arial, Helvetica, sans-serif;
    margin-left: 30px;
    text-decoration: line-through;
}

.detail-top .stock {
    float: right;
}

.detail-top .stock i {
    font-style: normal;
}

.select-spec {
    margin: 20px 0;
    background: #fff;
    padding: 30px;
    font-size: 30px;
    line-height: 1;
    position: relative;
}

