.store {
    margin: 20px;
    background: #fff;
    border-radius: 10px;
    /* margin-top: 139px; */
}

.store dt {
    margin-bottom: 20px;
    height: 90px;
    border-radius: 10px;
    background-color: #fff;
    -webkit-box-shadow: 0 2px 6px #eef1f0;
    -moz-box-shadow: 0 2px 6px #eef1f0;
    box-shadow: 0 2px 6px #eef1f0;
}

.store dt span {
    padding-left: 60px;
    display: block;
    float: left;
    width: 33.3333%;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    font-size: 24px;
    color: #90a4ae;
    line-height: 90px;
    position: relative;
}

.store dt span:before {
    position: absolute;
    top: 50%;
    left: 24px;
    display: block;
    content: "";
    width: 26px;
    height: 26px;
    background: url(../images/icon-auth.png) no-repeat;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}

.store dd {
    padding: 0 20px;
    height: 65px;
    line-height: 65px;
    position: relative;
}

.store dd span {
    margin-right: 32px;
    color: #cfd7d5;
    float: right;
}

.store dd:after {
    display: block;
    content: "";
    width: 12px;
    height: 22px;
    background: url(../images/arrow-right.png) no-repeat;
    position: absolute;
    top: 50%;
    right: 20px;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}

.detail-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
}

.detail-con {
    background: #fff;
}

.detail-con .tab li {
    float: left;
    width: 50%;
    line-height: 90px;
    font-size: 30px;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    text-align: center;
    border-bottom: 2px solid #eef1f0;
    position: relative;
}

.detail-con .tab .cur {
    color: #43cfa7;
}

.detail-con .tab .cur:after {
    display: block;
    content: "";
    width: 128px;
    height: 6px;
    background: #43cfa7;
    position: absolute;
    bottom: 0;
    left: 50%;
    -webkit-transform: translateX(-50%);
    transform: translateX(-50%);
}

.tab-con {
    display: block;
    float: left;
    height: auto;
    margin-bottom: 100px;
    padding: 20px 20px 40px;
}


/*购物车*/

.cart-dl {
    margin-bottom: 20px;
    background: #fff;
}

.cart-dl dt {
    padding: 0 20px;
    text-align: right;
    line-height: 90px;
    border-bottom: 1px solid #dde3e1;
}

.cart-dl dt span {
    float: left;
    display: block;
    position: relative;
    color: #90a4ae;
    font-weight: 700;
    padding: 0 32px 0 6px;
}

.cart-dl dt span:after {
    display: block;
    content: "";
    width: 12px;
    height: 22px;
    background: url(../images/arrow-right.png) no-repeat;
    position: absolute;
    top: 50%;
    right: 0;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}

.checkbox_selected {
    display: block;
    content: "";
    width: 36px;
    height: 36px;

    background-image:url(../images/icon-selected.png);
    background-repeat: no-repeat;
    background-size: cover;
    position: absolute;
    top: 50%;
    left: 0;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}

.checkbox_select {
    display: block;
    content: "";
    width: 36px;
    height: 36px;
    background-image:url(../images/icon-select.png);
    background-repeat: no-repeat;
    background-size: cover;
    position: absolute;
    top: 50%;
    left: 0;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}

.cart-dl dd i {
    left: -60px;
}

.cart-bottom .select-all i {
    left: 20px;
}

.cart-dl dt a {
    float: right;
    display: block;
    line-height: 90px;
    color: #333;
}

.cart-dl dd {
    min-height: 150px;
    margin-left: 80px;
    border-bottom: 1px solid #dde3e1;
    position: relative;
    padding: 30px 30px 30px 177px;
}



.cart-dl dd:last-of-type {
    border: none;
}

.cart-dl dd img {
    position: absolute;
    top: 30px;
    left: 0;
    display: block;
    width: 150px;
    height: 150px;
}

.cart-dl dd h5 {
    margin-top: -7px;
    height: 68px;
    margin-bottom: 10px;
    line-height: 34px;
}

.cart-dl dd .spec {
    margin-bottom: 10px;
    color: #90a4ae;
    font-size: 24px;
}

.cart-dl dd .price {
    text-align: right;
    font-family: Arial, Helvetica, sans-serif;
}

.cart-dl dd .price .pp {
    display: block;
    float: left;
    color: #ec4343;
}

.cart-dl dd .price .pp em {
    font-size: 36px;
}

.cart-bottom {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    height: 98px;
    line-height: 98px;
    background: #fff;
}

.cart-bottom .select-all {
    display: block;
    float: left;
    position: relative;
    padding-left: 80px;
    font-size: 30px;
}

.cart-bottom .btn-settle {
    float: right;
    width: 250px;
    height: 97px;
    line-height: 97px;
    background: #43cfa7;
    text-align: center;
    color: #fff;
    font-size: 36px;
}

.cart-bottom .total {
    margin-right: 50px;
    float: right;
    font-size: 30px;
}

.cart-bottom .total em,
.cart-bottom .total em i {
    font-family: Arial, Helvetica, sans-serif;
    font-style: normal;
    color: #ec4343;
}

.cart-dl .count-right {
    float: none;
    position: absolute;
    bottom: 20px;
    right: 20px;
}


/*订单详情*/

.order-status {
    margin-bottom: 20px;
    padding: 17px 20px 10px;
    background: #fff;
    font-size: 30px;
    line-height: 36px;
    position: relative;
}

.order-status dd {
    padding: 10px 0 10px 60px;
    position: relative;
}

.order-status .status span {
    color: #fbc02d;
}

.order-status:after {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    display: block;
    content: "";
    height: 4px;
    background: url(../images/bg-color.png) repeat-x;
    background-size: contain;
}

.order-status .status:before {
    position: absolute;
    top: 50%;
    left: 0;
    display: block;
    content: "";
    width: 36px;
    height: 36px;
    background: url(../images/icon-status.png) no-repeat;
    background-size: contain;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}

.order-status .address:before {
    position: absolute;
    top: 10px;
    left: 0;
    display: block;
    content: "";
    width: 36px;
    height: 36px;
    background: url(../images/icon-address.png) no-repeat;
    background-size: contain;
}

.order-status .address .tel {
    float: right;
    font-family: Arial, Helvetica, sans-serif;
}

.order-status .address p {
    margin-top: 10px;
    font-size: 24px;
    color: #90a4ae;
}

.order-list {
    margin-bottom: 20px;
    background: #fff;
}

.order-list h4 {
    border-bottom: 1px solid #dde3e1;
}

.order-list h4 span {
    float: left;
    padding: 0 30px;
    display: block;
    line-height: 70px;
    color: #90a4ae;
    position: relative;
}

.order-list h4 span:after {
    display: block;
    content: "";
    width: 12px;
    height: 22px;
    background: url(../images/arrow-right.png) no-repeat;
    position: absolute;
